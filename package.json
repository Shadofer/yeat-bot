{
  "name": "yeat",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npx prisma generate && npx prisma db push && swc src -d output && copyfiles generated/* generated/local/** output",
    "start": "run-s build start:cached",
    "start:cached": "node -r @swc-node/register -r tsconfig-paths/register output/src/main.js",
    "dev": "tsnd --loader ts-node/esm --ignore-watch generated/ --transpile-only --respawn --exit-child --clear -r tsconfig-paths/register src/main.ts",
    "check": "tsc"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.0",
    "@discordx/pagination": "^3.5.0",
    "@prisma/client": "^5.13.0",
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "^0.3.12",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.14.1",
    "discordx": "^11.9.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "imagekit": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.13.0",
    "qrcode": "^1.5.3",
    "querystring": "^0.2.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
